<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Geidea Payment Provider Data -->
        <record id="payment_provider_geidea" model="payment.provider">
            <field name="name">Geidea</field>
            <field name="code">geidea</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="state">disabled</field>
            <field name="is_published">False</field>
            <field name="geidea_test_mode">True</field>
            <field name="payment_icon_ids" eval="[(6, 0, [ref('payment.payment_icon_cc_visa'), ref('payment.payment_icon_cc_mastercard')])]"/>
        </record>
    </data>
</odoo>