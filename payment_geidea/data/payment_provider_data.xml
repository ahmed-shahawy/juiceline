<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Payment Provider: Geidea -->
        <record id="payment_provider_geidea" model="payment.provider">
            <field name="name">Geidea</field>
            <field name="code">geidea</field>
            <field name="state">disabled</field>
            <field name="is_published">False</field>
            <field name="payment_method_ids" eval="[(6, 0, [ref('payment.payment_method_card')])]"/>
            <field name="allow_tokenization">True</field>
            <field name="capture_manually">False</field>
            <field name="allow_express_checkout">False</field>
            <field name="redirect_form_view_id" ref="payment.redirect_form"/>
            <field name="inline_form_view_id" ref="payment.inline_form"/>
            <field name="token_inline_form_view_id" ref="payment.token_inline_form"/>
            <field name="token_redirect_form_view_id" ref="payment.token_redirect_form"/>
            <field name="fees_active">False</field>
            <field name="fees_dom_fixed">0.0</field>
            <field name="fees_dom_var">0.0</field>
            <field name="fees_int_fixed">0.0</field>
            <field name="fees_int_var">0.0</field>
            <field name="geidea_merchant_id"></field>
            <field name="geidea_api_key"></field>
            <field name="geidea_merchant_password"></field>
        </record>

    </data>
</odoo>