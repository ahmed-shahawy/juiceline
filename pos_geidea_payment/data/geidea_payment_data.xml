<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Geidea Payment Sequence -->
        <record id="sequence_geidea_payment" model="ir.sequence">
            <field name="name">Geidea Payment Transaction</field>
            <field name="code">geidea.payment</field>
            <field name="prefix">GDI</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Default Geidea Payment Method -->
        <record id="payment_method_geidea" model="pos.payment.method">
            <field name="name">Geidea Payment</field>
            <field name="is_cash_count">False</field>
            <field name="split_transactions">True</field>
            <field name="open_session_ids" eval="False"/>
        </record>

    </data>
</odoo>